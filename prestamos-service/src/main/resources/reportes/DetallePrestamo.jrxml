	<parameter name="id_prestamo_buscar" class="java.lang.Integer"/>
	<queryString language="SQL">
		<![CDATA[SELECT 
    u.id_usuario, u.nombre_usuario, u.apellido_usuario, u.dni,
    u.correo, u.telefono, u.direccion, u.genero, u.fecha_nacimiento,
    l.titulo, l.anio, l.isbn, l.editorial,
    a.nombre_autores, a.nacionalidad,
    c.nombre_categoria,
    p.fecha_prestamo, p.fecha_devolucion, p.fecha_real, p.multa, p.comentarios,
    e.descripcion AS estado_libro
FROM prestamos p
INNER JOIN usuario u ON p.id_usuario = u.id_usuario
INNER JOIN libros l ON p.id_libros = l.id_libros
INNER JOIN autores a ON l.id_autores = a.id_autores
INNER JOIN categoria c ON l.id_categoria = c.id_categoria
INNER JOIN estado_libro e ON p.id_estado = e.id_estado
WHERE ($P{id_prestamo_buscar} IS NULL) OR (p.id_prestamos = $P{id_prestamo_buscar})]]>
	</queryString>
